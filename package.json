{
  "name": "react-redux-typescript-seed",
  "private": false,
  "description": "A seed project for development in Typescript for React using Redux and Redux-Saga",
  "repository": "",
  "version": "0.3.100",
  "main": "lib/package.js",
  "types": "lib/package.d.ts",
  "files": [
    "lib",
    "lib/stylesheets",
    "assets"
  ],
  "scripts": {
    "start": "nodemon --ext js,ts,tsx,jsx,json -w ./  --ignore ./public/ --ignore ./src/ --ignore graphQL/ devServer.js --ignore nightwatch/ --ignore build/messages/  --ignore build/ --ignore extracted/ --config config/webpack.config.js",
    "build": "cross-env NODE_ENV=development webpack -d --config config/webpack.config.build.js --bail",
    "compiletests": "cross-env NODE_ENV=test webpack -d --config config/webpack.config.test.js --bail",
    "compiletests-end": "cross-env NODE_ENV=test webpack -d --config config/webpack.config.test.endtoend.js --bail",
    "compiletestswatch": "npm run compiletests -- --watch",
    "pretest": "tsc -p tsconfig_testPreProcessor.json",
    "test": "jest --no-cache --config jestConfig.json",
    "pretest:watch": "tsc -p tsconfig_testPreProcessor.json",
    "test:watch": "npm run test -- --watch",
    "testlogserver": "node ./nightwatch/startTestLogServer.js",
    "nightwatch": "./node_modules/.bin/nightwatch",
    "eslint": "eslint --ext .js,.jsx .",
    "eslintfix": "eslint --fix --ext .js,.jsx .",
    "tslint": "tslint -c tslint.json --project tsconfig.json",
    "tslintfix": "tslint --fix -c tslint.json --project tsconfig.json",
    "postinstall": "rimraf typings/globals & rimraf typings/modules & rimraf typings/index.d.ts & typings install"
  },
  "devDependencies": {
