{
  "name": "react-autodropdown",
  "private": false,
  "description": "A simple autodropdown",
  "repository": "",
  "version": "0.3.580",
  "main": "lib/package.js",
  "types": "lib/package.d.ts",
  "files": [
    "lib",
    "lib/stylesheets",
    "assets"
  ],
  "scripts": {
    "start": "nodemon --ext js,ts,tsx,jsx,json -w ./  --ignore ./public/ --ignore ./src/ --ignore graphQL/ devServer.js --ignore nightwatch/ --ignore build/messages/  --ignore build/ --ignore extracted/ --config config/webpack.config.js",
    "build": "cross-env NODE_ENV=development webpack -d --config webpack.config.build.js --bail",
    "eslint": "eslint --ext .js,.jsx .",
    "eslintfix": "eslint --fix --ext .js,.jsx .",
    "tslint": "tslint -c tslint.json --project tsconfig.json",
    "tslintfix": "tslint --fix -c tslint.json --project tsconfig.json",
    "postinstall": "rimraf typings/globals & rimraf typings/modules & rimraf typings/index.d.ts & typings install"
  },
  "devDependencies": {
    "@types/classnames": "2.2.7",
    "@types/node": "11.11.4",
    "@types/react": "16.8.4",
    "@types/react-dom": "16.8.2",
    "autoprefixer": "9.5.0",
    "awesome-typescript-loader": "5.2.1",
    "babel-core": "6.26.3",
    "babel-loader": "8.0.5",
    "babel-plugin-react-intl": "3.0.1",
    "core-js": "2.6.5",
    "cross-env": "5.2.0",
    "css-loader": "1.0.1",
    "eslint": "5.14.1",
    "eslint-config-google": "0.12.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.12.4",
    "eventsource-polyfill": "0.9.6",
    "express": "4.16.4",
    "file-loader": "3.0.1",
    "glob": "7.1.3",
    "ignore-loader": "0.1.2",
    "node-sass": "^4.11.0",
    "postcss-custom-properties": "8.0.9",
    "postcss-loader": "3.0.0",
    "progress-bar-webpack-plugin": "1.12.1",
    "resolve-url-loader": "3.0.1",
    "sass-loader": "7.1.0",
    "sass-resources-loader": "2.0.0",
    "style-loader": "0.23.1",
    "tslint": "5.13.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-loader": "3.5.4",
    "typescript": "3.3.3333",
    "url-loader": "1.1.2",
    "webpack": "4.29.5",
    "webpack-cli": "3.2.3",
    "webpack-config": "7.5.0",
    "webpack-dev-middleware": "3.6.0",
    "webpack-hot-middleware": "2.24.3"
  },
  "dependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "classnames": "2.2.6",
    "html-webpack-plugin": "3.2.0",
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "react-polyfill": "0.0.2"
  }
}
